{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/chat-test/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/chat-test/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/chat-test",
  "projects": {
    "chat-test": {
      "type": "application",
      "root": "apps/chat-test",
      "entryFile": "main",
      "sourceRoot": "apps/chat-test/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chat-test/tsconfig.app.json"
      }
    },
    "broker": {
      "type": "application",
      "root": "apps/broker",
      "entryFile": "main",
      "sourceRoot": "apps/broker/src",
      "compilerOptions": {
        "tsConfigPath": "apps/broker/tsconfig.app.json"
      }
    }
  }
}